YUI.add("cookie-banner-view",(function(e,i){require("hermes-core/flog")(i);e.FlickrView.create(this.name,e.FlickrView,[],{langBundles:this.details.langBundles,initializer:function(e){this.params=e},loadState:function(){return e.Promise.resolve()},buildContainer:function(){var e="<p>"+this.intlMessage({intlName:"privacy.COOKIE_BANNER_TEXT",url:"/help/cookies"})+"</p>";return this.setContainerWithTemplate("basic-banner",{message:e,additionalClasses:"foot-banner-style"}),this},activate:function(){var e=this.get("container");return this.registerEventHandler(e.one(".button.dismiss").on("click",this.dismissBanner.bind(this))),this.fire("activated"),this},dismissBanner:function(i){var s=new Date;s.setTime(s.getTime()+31536e6),this.appContext.cookieCatcher.setCookieIfAllowed(e.config.flickr.banners.dismiss_cookie_banner_name,1,{expires:s,path:"/"}),this.fire("dismiss")}})}),"@VERSION@",{requires:["flickr-view","hermes-template-basic-banner","user-limits-helper","person-models","person-profile-models"],optionalRequires:["hermes-core"],langBundles:["privacy"]});YUI.add("hermes-lang-privacy_es-us",(function(e,a){e.Intl.add("hermes/privacy","es-US",{OATH_DATA_USAGE:[{type:0,value:"Flickr y Yahoo ahora forman parte de “Oath” y son miembros de la familia de empresas de Verizon. A partir del 15 de septiembre de 2017, planeamos compartir información para el usuario sobre nuestra nueva familia."}],SM:[{type:0,value:"Noticias emocionantes: ¡Flickr se unirá a SmugMug!"}],COOKIE_BANNER_TEXT:[{type:0,value:'Este sitio usa cookies para mejorar tu experiencia y mostrarte contenido más relevante de acuerdo con tus intereses. Al usar este sitio, aceptas que Flickr y nuestros asociados usen cookies según lo describimos en nuestra <a target="_blank" href="'},{type:1,value:"url"},{type:0,value:'">política de cookies</a>.'}]})}),"@VERSION@",{requires:["intl"]});